{
  "name": "warehouse/repository",
  "description": "Generic implementation of Domain-Driven Design Repository Pattern",
  "type": "library",
  "license": "mit",
  "authors": [
    {
      "name": "Flavio Heleno",
      "email": "flaviohbatista@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Warehouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warehouse\\Test\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
